    body
        .navbar.navbar-default
            .container-fluid
                .navbar-header
                    button.navbar-toggle(type="button", data-toggle="collapse", data-target="#mynavbar-content")
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    //a.navbar-brand(href="#") SitePoint
                #mynavbar-content.collapse.navbar-collapse
                    ul.nav.navbar-nav
                        li.active
                            a(href="#") Home
                        li
                            a(href="#") About
                        li
                            a(href="#") Pricing
                        li
                            a(href="#") Contact
                        li
                            a(href="#") Feedbak


        .container
            .page-header
                h1 Header
        .container
            .row
                .col-xs-9.col-xs-push-3.text-center(style="background: #5C7080;")
                    h1 Column 1
                .col-xs-3.col-xs-pull-9.text-center(style="background: #6BC0FF;")
                    h1 Column 2
            .row
                .col-xs-3.col-xs-offset-2.text-center(style="background: #E8AA4C;")
                    h1 Column 3
                .col-xs-3.text-center(style="background: #FF384E;")
                    h1 Column 4
        .container
            .panel.panel-primary
                .panel-heading ATTENTION
                .panel-body AAAAAA
                .panel-footer
                    a.btn.btn-danger.btn-sm(href="#") Agree
                    a.btn.btn-default.btn-sm(href="#") Decline
